tersebut.</idx-cor:AccountingStandardsIssuedButNotYetEffectiveTextBlock><idx-cor:ConsumerFinancingLiabilitiesTextBlock id="IXF2610000E02_1334_00002_00_0001" contextRef="CurrentYearDuration">-</idx-cor:ConsumerFinancingLiabilitiesTextBlock><idx-cor:PropertyPlantAndEquipment id="IXF2611000E02_0887_00216_01_0001" decimals="-3" contextRef="PriorEndYearInstant_2611000_CarryingAmountGrossMember_LandDirectlyOwnedMember" unitRef="IDR">22914728000</idx-cor:PropertyPlantAndEquipment><idx-cor:AdditionInPropertyPlantAndEquipment id="IXF2611000E02_1355_00142_01_0001" decimals="-3" contextRef="CurrentYearDuration_2611000_CarryingAmountGrossMember_LandDirectlyOwnedMember" unitRef="IDR">624404000</idx-cor:AdditionInPropertyPlantAndEquipment><idx-cor:DisposalsInPropertyPlantAndEquipment id="IXF2611000E02_1356_00142_01_0001" xsi:nil="true" contextRef="CurrentYearDuration_2611000_CarryingAmountGrossMember_LandDirectlyOwnedMember" unitRef="IDR"/><idx-cor:ReclassificationsOfPropertyPlantAndEquipment id="IXF2611000E02_1357_00142_01_0001" decimals="-3" contextRef="CurrentYearDuration_2611000_CarryingAmountGrossMember_LandDirectlyOwnedMember" unitRef="IDR">16004548000</idx-cor:ReclassificationsOfPropertyPlantAndEquipment><idx-cor:TranslationAdjustmentPropertyPlantandEquipment id="IXF2611000E02_1406_00142_01_0001" xsi:nil="true" contextRef="CurrentYearDuration_2611000_CarryingAmountGrossMember_LandDirectlyOwnedMember" unitRef="IDR"/><idx-cor:NewSubsidiariesInPropertyPlantAndEquipment id="IXF2611000E02_1358_00142_01_0001" xsi:nil="true" contextRef="CurrentYearDuration_2611000_CarryingAmountGrossMember_LandDirectlyOwnedMember" unitRef="IDR"/><idx-cor:RevaluationOfPropertyPlantAndEquipment id="IXF2611000E02_1359_00142_01_0001" xsi:nil="true" contextRef="CurrentYearDuration_2611000_CarryingAmountGrossMember_LandDirectlyOwnedMember" unitRef="IDR"/><idx-cor:ImpairmentRecoveryOfPropertyPlantAndEquipment id="IXF2611000E02_1360_00142_01_0001" xsi:nil="true" contextRef="CurrentYearDuration_2611000_Ca