" contextRef="PriorYearDuration" unitRef="IDR"/><idx-cor:ProceedsFromSalesOfInterestsInSubsidiariesWithoutLossOfControlFromFinancingActivities id="IXF1510000E02_0438_00002_01_0001" xsi:nil="true" contextRef="CurrentYearDuration" unitRef="IDR"/><idx-cor:ProceedsFromSalesOfInterestsInSubsidiariesWithoutLossOfControlFromFinancingActivities id="IXF1510000E02_0438_00006_01_0001" xsi:nil="true" contextRef="PriorYearDuration" unitRef="IDR"/><idx-cor:ProceedFromIncreaseOfNonControllingInterest id="IXF1510000E02_0364_00002_01_0001" xsi:nil="true" contextRef="CurrentYearDuration" unitRef="IDR"/><idx-cor:ProceedFromIncreaseOfNonControllingInterest id="IXF1510000E02_0364_00006_01_0001" xsi:nil="true" contextRef="PriorYearDuration" unitRef="IDR"/><idx-cor:PaymentsForAcquisitionOfNoncontrollingInterestsInSubsidiaries id="IXF1510000E02_0344_00002_01_0001" xsi:nil="true" contextRef="CurrentYearDuration" unitRef="IDR"/><idx-cor:PaymentsForAcquisitionOfNoncontrollingInterestsInSubsidiaries id="IXF1510000E02_0344_00006_01_0001" xsi:nil="true" contextRef="PriorYearDuration" unitRef="IDR"/><idx-cor:DividendsPaidFromFinancingActivities id="IXF1510000E02_0129_00002_01_0001" decimals="-6" contextRef="CurrentYearDuration" unitRef="IDR">714160000000</idx-cor:DividendsPaidFromFinancingActivities><idx-cor:DividendsPaidFromFinancingActivities id="IXF1510000E02_0129_00006_01_0001" decimals="-6" contextRef="PriorYearDuration" unitRef="IDR">555460000000</idx-cor:DividendsPaidFromFinancingActivities><idx-cor:InterestsReceivedFromFinancingActivities id="IXF1510000E02_0188_00002_01_0001" xsi:nil="true" contextRef="CurrentYearDuration" unitRef="IDR"/><idx-cor:InterestsReceivedFromFinancingActivities id="IXF1510000E02_0188_00006_01_0001" xsi:nil="true" contextRef="PriorYearDuration" unitRef="IDR"/><idx-cor:InterestsPaidFromFinancingActivities id="IXF1510000E02_0185_00002_01_0001" decimals="-6" contextRef="CurrentYearDuration" unitRef="IDR">1234000000</idx-cor:InterestsPaidFromFinancingActivities><idx-c